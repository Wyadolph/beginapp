body{	/* Setting default text color, background and a font stack */	/*	color:#444444;	font-size:13px;	background: #f2f2f2;	font-family:Arial, Helvetica, sans-serif;*/}#gallery{	/* CSS3 Box Shadow */	/*	-moz-box-shadow:0 0 3px #AAAAAA;	-webkit-box-shadow:0 0 3px #AAAAAA;	*/	box-shadow:0 0 3px #AAAAAA;		/* CSS3 Rounded Corners */	/*	-moz-border-radius-bottomleft:4px;	-webkit-border-bottom-left-radius:4px;	border-bottom-left-radius:4px;		-moz-border-radius-bottomright:4px;	-webkit-border-bottom-right-radius:4px;	*/	border-bottom-right-radius:4px;		border:1px solid white;			/* The width of the gallery */	width:748px;	overflow:hidden;}#slides{width:2280px;margin-left:-748px;overflow:hidden;}#slides .slide{width:748px;float:left;height: 896px;}#menu{padding: 2px;background-color: #EEFFEA;position: absolute;z-index: 1;width: 745px;border-bottom: 1px solid green;}ul{	margin:2px;	padding:0px;	text-align:center;}li{	/* Every thumbnail is a li element */	width:60px;	display:inline-block;	list-style:none;	height:45px;	overflow:hidden;}li.act{cursor:default;}li.inact{cursor:pointer;}.radius{border-radius:5px;}